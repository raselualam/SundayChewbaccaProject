{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "id",
        "username",
        "firstName",
        "lastName",
        "email",
        "password",
        "phone",
        "userStatus"
    ],
    "properties": {
        "id": {
            "type": "integer",
            "default": 0,
            "title": "The id Schema",
            "examples": [
                9222968140497193723
            ]
        },
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "rasel"
            ]
        },
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "mohd"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "alam"
            ]
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema",
            "examples": [
                "rasel@gmail.com"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "12345"
            ]
        },
        "phone": {
            "type": "string",
            "default": "",
            "title": "The phone Schema",
            "examples": [
                "9762584586"
            ]
        },
        "userStatus": {
            "type": "integer",
            "default": 0,
            "title": "The userStatus Schema",
            "examples": [
                1
            ]
        }
    },
    "examples": [{
        "id": 9222968140497193723,
        "username": "rasel",
        "firstName": "mohd",
        "lastName": "alam",
        "email": "rasel@gmail.com",
        "password": "12345",
        "phone": "9762584586",
        "userStatus": 1
    }]
}